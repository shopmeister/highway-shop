<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ShopmasterZalandoConnectorSix\Subscribers\Curl\CurlExceptionSubscriber">
            <tag name="kernel.event_subscriber"/>
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\HttpClient\ClientService"/>
            <argument type="service"
                      id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Auth\ApiZalandoAuthService"/>
        </service>

        <service id="ShopmasterZalandoConnectorSix\Subscribers\Order\OrderStateMachineTransitionSubscriber">
            <tag name="kernel.event_subscriber"/>
            <argument type="service" id="order_delivery.repository"/>
            <argument type="service" id="messenger.bus.shopware"/>
        </service>

        <service id="ShopmasterZalandoConnectorSix\Subscribers\Order\LineItemReturnedSubscriber">
            <tag name="kernel.event_subscriber"/>
            <argument type="service" id="messenger.bus.shopware"/>
        </service>

    </services>
</container>