        <service id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Product\ApiZalandoProductStockService">
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\HttpClient\ClientService"/>
        </service>

        <service id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Product\ApiZalandoProductPriceService">
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\HttpClient\ClientService"/>
        </service>

        <service id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Product\ApiZalandoProductPriceAttemptService">
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\HttpClient\ClientService"/>
            <argument type="service" id="zalando_price_report.repository"/>
