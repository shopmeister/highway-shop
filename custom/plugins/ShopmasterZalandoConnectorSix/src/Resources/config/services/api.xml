            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\HttpClient\ClientService"/>
        </service>

        <!--        <service id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Order\ApiZalandoOrderImportService"/>-->


    </services>
