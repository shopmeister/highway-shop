        </service>

        <service id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Outline\ApiZalandoOutlineService">
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\HttpClient\ClientService"/>
        </service>

