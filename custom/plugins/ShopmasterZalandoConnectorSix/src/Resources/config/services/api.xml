            <argument type="service" id="ShopmasterZalandoConnectorSix\Logger"/>
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\HttpClient\ClientService"/>
            <argument type="service" id="messenger.bus.shopware"/>
