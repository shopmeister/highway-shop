            </call>
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Outline\ApiZalandoOutlineService"/>
        </service>

        <service id="ShopmasterZalandoConnectorSix\Controllers\Api\Zalando\AuthZalandoController">
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Auth\ApiZalandoAuthService"/>
        </service>

        <service id="ShopmasterZalandoConnectorSix\Controllers\Api\Zalando\OrderZalandoController">
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\Api\Zalando\Order\ApiZalandoOrderService"/>
            <argument type="service" id="ShopmasterZalandoConnectorSix\Services\Order\Backend\OrderDataBackendService"/>
