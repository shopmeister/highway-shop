{
  "name": "@magnalister/amazon-variations-react",
  "version": "1.0.0",
  "description": "React component for Amazon attribute variations matching - Magnalister integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types && npm run move-css",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "move-css": "mkdir -p ../../css/react/dist && cp -f dist/style.css ../../css/react/dist/ 2>/dev/null || echo 'No CSS file to move'",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.8.1",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-select": "^5.10.2",
    "react-window": "^1.8.8",
    "utils": "^0.3.1"
  },
