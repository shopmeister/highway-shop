{% set pickwareDhlRadioGroupId = random() %}

{% set pickwareDhlLogo %}
    <img
        class="pickware-dhl-address-radio-logo"
        src="data:image/png;base64,"
        alt=""
        role="presentation"
    />
{% endset %}

<div
    class="pickware-dhl-special-address-radio-group pickware-dhl-address-radio-group"
    pickware-dhl-address-form
>
    <div>
        <input
            type="radio"
            value="regular"
            name="pickwareDhlAddressRadioGroup"
            class="pickware-dhl-radio-button-regular"
            id="{{ pickwareDhlRadioGroupId }}pickwareDhlAddressRadioButtonRegular"
        >

        <label for="{{ pickwareDhlRadioGroupId }}pickwareDhlAddressRadioButtonRegular">
            {{ 'pickware-dhl.special-addresses.storefront.regular.radio-button-label'|trans|sw_sanitize }}
        </label>
    </div>

    {% if pickwareDhlEnablePackstation %}
    <div>
        <input
            type="radio"
            value="packstation"
            name="pickwareDhlAddressRadioGroup"
            class="pickware-dhl-radio-button-packstation"
            id="{{ pickwareDhlRadioGroupId }}pickwareDhlAddressRadioButtonPackstation"
        >

        <label for="{{ pickwareDhlRadioGroupId }}pickwareDhlAddressRadioButtonPackstation">
            {{ 'pickware-dhl.special-addresses.storefront.packstation.radio-button-label'|trans|sw_sanitize }}
            {{ pickwareDhlLogo }}
        </label>
    </div>
    {% endif %}
    {% if pickwareDhlEnablePostOffice %}
    <div>
        <input
            type="radio"
            value="post-office"
            name="pickwareDhlAddressRadioGroup"
            class="pickware-dhl-radio-button-post-office"
            id="{{ pickwareDhlRadioGroupId }}pickwareDhlAddressRadioButtonPostOffice"
        >

        <label for="{{ pickwareDhlRadioGroupId }}pickwareDhlAddressRadioButtonPostOffice">
            {{ 'pickware-dhl.special-addresses.storefront.post-office.radio-button-label'|trans|sw_sanitize }}
            {{ pickwareDhlLogo }}
        </label>
    </div>
    {% endif %}
    {% if pickwareDhlEnablePaketshop %}
        <div>
            <input
                type="radio"
                value="paketshop"
                name="pickwareDhlAddressRadioGroup"
                class="pickware-dhl-radio-button-paketshop"
                id="{{ pickwareDhlRadioGroupId }}pickwareDhlAddressRadioButtonPaketshop"
            >

            <label for="{{ pickwareDhlRadioGroupId }}pickwareDhlAddressRadioButtonPaketshop">
                {{ 'pickware-dhl.special-addresses.storefront.paketshop.radio-button-label'|trans|sw_sanitize }}
                {{ pickwareDhlLogo }}
            </label>
        </div>
    {% endif %}
</div>
