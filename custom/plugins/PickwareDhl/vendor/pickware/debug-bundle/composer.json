{
    "name": "pickware/debug-bundle",
    "description": "A bundle for Shopware 6 that provides helpful utilities for debugging.",
    "license": "proprietary",
    "type": "library",
    "version": "4.26.0",
    "authors": [
        {
            "name": "Pickware GmbH",
            "email": "info@pickware.de"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "^3.6 || ^4.0",
        "guzzlehttp/psr7": "^2.4.5",
        "lcobucci/clock": "^2.0.0 || ^3.0.0",
        "lcobucci/jwt": "^5.0",
        "league/oauth2-server": "^8.2.4 || ^9.0",
        "nyholm/psr7": "^1.4.0",
        "pickware/dal-bundle": "^5.22.0",
        "pickware/http-utils": "^4.14.0",
        "pickware/php-standard-library": "^2.6.0",
        "psr/log": "^3.0.0",
        "shopware/core": "~6.6.2 || ~6.7.0",
        "symfony/config": "^7.0.0",
        "symfony/dependency-injection": "^7.0.0",
        "symfony/event-dispatcher": "^7.0.0",
        "symfony/http-foundation": "^7.0.0",
        "symfony/http-kernel": "^7.0.0",
        "symfony/psr-http-message-bridge": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "pickware/shopware-integration-test-plugin": "@dev",
        "pickware/unit-testing-tools": "*@dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../bundles/*"
        },
        {
            "type": "path",
            "url": "../../libraries/composer-packages/*"
        },
        {
            "type": "path",
            "url": "../../libraries-dev/composer-packages/*"
        },
        {
            "type": "path",
            "url": "../../plugins-dev/*"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pickware\\DebugBundle\\": "src"
        },
        "files": [
            "src/PickwareDebugging.php",
            "src/Profiling/Profiling.php"
        ],
        "exclude-from-classmap": [
            "**.unit.php",
            "**.integration.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pickware/dependency-loader": true,
            "symfony/runtime": true,
            "viison/composer-git-hooks-installer-plugin": true
        },
        "lock": false
    },
    "extra": {
        "ci-min-supported-shopware-version": "6.6.2.0"
    },
    "scripts": {
        "test:integration": "${SHOPWARE_DIR}/vendor/bin/phpunit --config phpunit.integration.xml --testdox",
        "test:unit": "./vendor/bin/phpunit --config phpunit.unit.xml --testdox"
    }
}
