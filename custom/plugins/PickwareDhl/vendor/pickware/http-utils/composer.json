{
    "name": "pickware/http-utils",
    "description": "A library with utils for HTTP request / response processing",
    "license": "proprietary",
    "type": "library",
    "version": "4.14.0",
    "authors": [
        {
            "name": "Pickware",
            "email": "info@pickware.de"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "ext-json": "*",
        "pickware/php-standard-library": "^2.6.0",
        "shopware/core": "~6.6.2 || ~6.7.0",
        "symfony/http-foundation": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../libraries/composer-packages/*"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pickware\\HttpUtils\\": "src",
            "Pickware\\HttpUtils\\Test\\": "test"
        },
        "exclude-from-classmap": [
            "**.unit.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "symfony/runtime": true,
            "viison/composer-git-hooks-installer-plugin": true
        },
        "lock": false
    },
    "extra": {
        "ci-min-supported-shopware-version": "6.6.2.0"
    },
    "scripts": {
        "test:unit": "./vendor/bin/phpunit --config phpunit.unit.xml --testdox"
    }
}
