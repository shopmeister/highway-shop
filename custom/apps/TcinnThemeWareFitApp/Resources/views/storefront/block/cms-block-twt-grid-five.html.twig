{# ThemeWareÂ® CMS block "Grid 'Five'" #}

{% block block_grid_five %}
    {% set columns = 2 %}
    {# @deprecated tag:v6.5.0 - Use "element.id" directly instead. #}
    {% set id = element.id %}

    {% block block_grid_five_left %}
        <div class="col-md-6">
            {% block block_grid_five_left_top %}
                {% set element = block.slots.getSlot('left-top') %}

                <div class="left-top" data-cms-element-id="{{ element.id }}">
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                </div>
            {% endblock %}

            {% block block_grid_five_left_bottom %}
                <div class="left-bottom row cms-row">
                    {% block block_grid_five_left_bottom_left %}
                        {% set element = block.slots.getSlot('left-bottom_left') %}

                        <div class="left-bottom left column col-sm-6" data-cms-element-id="{{ element.id }}">
                            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                        </div>
                    {% endblock %}

                    {% block block_grid_five_left_bottom_right %}
                        {% set element = block.slots.getSlot('left-bottom_right') %}

                        <div class="left-bottom right column col-sm-6" data-cms-element-id="{{ element.id }}">
                            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                        </div>
                    {% endblock %}
                </div>
            {% endblock %}
        </div>
    {% endblock %}

    {% block block_grid_five_right %}
        <div class="col-md-6">
            {% block block_grid_five_right_top %}
                {% set element = block.slots.getSlot('right-top') %}

                <div class="right-top" data-cms-element-id="{{ element.id }}">
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                </div>
            {% endblock %}

            {% block block_grid_five_right_bottom %}
                {% set element = block.slots.getSlot('right-bottom') %}

                <div class="right-bottom" data-cms-element-id="{{ element.id }}">
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                </div>
            {% endblock %}
        </div>
    {% endblock %}
{% endblock %}